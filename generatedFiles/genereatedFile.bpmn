<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    id="Definitions_1"
    targetNamespace="http://bpmn.io/schema/bpmn"
    exporter="Camunda Modeler"
    exporterVersion="3.2.2">
    <process id="Process_1" isExecutable="true">
        <!-- Process elements -->
        <startEvent id="StartEvent_1">
            <outgoing>SequenceFlow_1</outgoing>
        </startEvent>
        <task id="Task_FeasibilityCheck" name="Feasibility Check">
            <incoming>SequenceFlow_1</incoming>
            <outgoing>SequenceFlow_2</outgoing>
        </task>
        <exclusiveGateway id="ExclusiveGateway_1">
            <incoming>SequenceFlow_2</incoming>
            <outgoing>SequenceFlow_3</outgoing>
            <outgoing>SequenceFlow_4</outgoing>
        </exclusiveGateway>
        <task id="Task_CheckAvailability" name="Check Personnel Availability">
            <incoming>SequenceFlow_3</incoming>
            <outgoing>SequenceFlow_5</outgoing>
        </task>
        <task id="Task_CreateOffer" name="Create Offer">
            <incoming>SequenceFlow_5</incoming>
            <outgoing>SequenceFlow_6</outgoing>
        </task>
        <endEvent id="EndEvent_CreateOffer">
            <incoming>SequenceFlow_6</incoming>
        </endEvent>
        <endEvent id="EndEvent_InformCustomer">
            <incoming>SequenceFlow_4</incoming>
        </endEvent>
        <!-- Sequence flows -->
        <sequenceFlow id="SequenceFlow_1" sourceRef="StartEvent_1" targetRef="Task_FeasibilityCheck" />
        <sequenceFlow id="SequenceFlow_2" sourceRef="Task_FeasibilityCheck"
            targetRef="ExclusiveGateway_1" />
        <sequenceFlow id="SequenceFlow_3" sourceRef="ExclusiveGateway_1"
            targetRef="Task_CheckAvailability">
            <conditionExpression xsi:type="tFormalExpression">${feasible}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="SequenceFlow_4" sourceRef="ExclusiveGateway_1"
            targetRef="EndEvent_InformCustomer">
            <conditionExpression xsi:type="tFormalExpression">${not feasible}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="SequenceFlow_5" sourceRef="Task_CheckAvailability"
            targetRef="Task_CreateOffer" />
        <sequenceFlow id="SequenceFlow_6" sourceRef="Task_CreateOffer"
            targetRef="EndEvent_CreateOffer" />
        <!-- BPMN diagram -->
        <bpmndi:BPMNDiagram id="BPMNDiagram_1">
            <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Process_1">
                <!-- Start event shape -->
                <bpmndi:BPMNShape id="_BPMNShape_StartEvent_1" bpmnElement="StartEvent_1">
                    <dc:Bounds x="173" y="102" width="36" height="36" />
                </bpmndi:BPMNShape>
                <!-- Feasibility Check task shape -->
                <bpmndi:BPMNShape id="Task_FeasibilityCheck_di" bpmnElement="Task_FeasibilityCheck">
                    <dc:Bounds x="250" y="80" width="100" height="80" />
                </bpmndi:BPMNShape>
                <!-- Exclusive Gateway shape -->
                <bpmndi:BPMNShape id="ExclusiveGateway_1_di" bpmnElement="ExclusiveGateway_1">
                    <dc:Bounds x="400" y="100" width="50" height="50" />
                </bpmndi:BPMNShape>
                <!-- Check Availability task shape -->
                <bpmndi:BPMNShape id="Task_CheckAvailability_di"
                    bpmnElement="Task_CheckAvailability">
                    <dc:Bounds x="550" y="80" width="100" height="80" />
                </bpmndi:BPMNShape>
                <!-- Create Offer task shape -->
                <bpmndi:BPMNShape id="Task_CreateOffer_di" bpmnElement="Task_CreateOffer">
                    <dc:Bounds x="700" y="80" width="100" height="80" />
                </bpmndi:BPMNShape>
                <!-- End Event (Create Offer) shape -->
                <bpmndi:BPMNShape id="EndEvent_CreateOffer_di" bpmnElement="EndEvent_CreateOffer">
                    <dc:Bounds x="850" y="102" width="36" height="36" />
                </bpmndi:BPMNShape>
                <!-- End Event (Inform Customer) shape -->
                <bpmndi:BPMNShape id="EndEvent_InformCustomer_di"
                    bpmnElement="EndEvent_InformCustomer">
                    <dc:Bounds x="550" y="220" width="36" height="36" />
                </bpmndi:BPMNShape>
                <!-- Sequence flows -->
                <bpmndi:BPMNEdge id="SequenceFlow_1_di" bpmnElement="SequenceFlow_1">
                    <di:waypoint x="209" y="120" />
                    <di:waypoint x="250" y="120" />
                </bpmndi:BPMNEdge>
                <bpmndi:BPMNEdge id="SequenceFlow_2_di" bpmnElement="SequenceFlow_2">
                    <di:waypoint x="350" y="120" />
                    <di:waypoint x="400" y="120" />
                </bpmndi:BPMNEdge>
                <bpmndi:BPMNEdge id="SequenceFlow_3_di" bpmnElement="SequenceFlow_3">
                    <di:waypoint x="450" y="145" />
                    <di:waypoint x="550" y="145" />
                </bpmndi:BPMNEdge>
                <bpmndi:BPMNEdge id="SequenceFlow_4_di" bpmnElement="SequenceFlow_4">
                    <di:waypoint x="425" y="95" />
                    <di:waypoint x="425" y="220" />
                    <di:waypoint x="550" y="220" />
                </bpmndi:BPMNEdge>
                <bpmndi:BPMNEdge id="SequenceFlow_5_di" bpmnElement="SequenceFlow_5">
                    <di:waypoint x="650" y="120" />
                    <di:waypoint x="700" y="120" />
                </bpmndi:BPMNEdge>
                <bpmndi:BPMNEdge id="SequenceFlow_6_di" bpmnElement="SequenceFlow_6">
                    <di:waypoint x="800" y="120" />
                    <di:waypoint x="850" y="120" />
                </bpmndi:BPMNEdge>
                <bpmndi:BPMNEdge id="SequenceFlow_7_di" bpmnElement="SequenceFlow_7">
                    <di:waypoint x="625" y="145" />
                    <di:waypoint x="700" y="145" />
                </bpmndi:BPMNEdge>
                <!-- End of Sequence flows -->
            </bpmndi:BPMNPlane>
        </bpmndi:BPMNDiagram>
    </process>
</definitions>